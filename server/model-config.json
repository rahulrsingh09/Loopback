{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "localmysql",
    "public": true
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "Message": {
    "dataSource": null
  },
  "CoffeeShop": {
    "dataSource": "localmysql",
    "public": true
  },
  "Review": {
    "dataSource": "localmysql",
    "public": true
  },
  "Reviewer": {
    "dataSource": "localmysql",
    "public": true
  },
  "test": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "greet": true
        }
      }
    }
  },
  "clients": {
    "dataSource": "localmysql",
    "public": true
  }
}
